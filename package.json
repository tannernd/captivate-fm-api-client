{
  "name": "captivate-fm-api-client",
  "version": "0.1.0",
  "description": "A Node.js client for the Captivate.fm API. Upload media, create episodes, and manage authentication.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "exports": {
    "require": "./dist/index.cjs.js",
    "import": "./dist/index.esm.js"
  },
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "captivate",
    "podcast",
    "api",
    "client",
    "uploader",
    "episodes",
    "node"
  ],
  "author": "Nate Tanner",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "form-data": "^4.0.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.48"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "rollup": "^4.0.0"
  }
}
